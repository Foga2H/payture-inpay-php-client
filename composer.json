{
  "name": "foga2h/payture-inpay-client",
  "license": "MIT",
  "description": "Client for Payture InPay API",
  "keywords": [
    "payture",
    "payment",
    "api",
    "client"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Lamoda developers",
      "homepage": "https://tech.lamoda.ru/"
    }
  ],
  "autoload": {
