  "require": {
    "php": "^8.2",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.4.5",
    "psr/log": "^1.1",
    "paillechat/php-enum": "^1.2"
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-json": "*",
    "phpunit/phpunit": "^8.5",
    "friendsofphp/php-cs-fixer": "^2.14"
